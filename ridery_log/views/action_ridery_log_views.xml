<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="ridery_log_tree_view" model="ir.ui.view">
            <field name="name">ridery_log_tree_view</field>
            <field name="model">ridery.log</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="create_date"/>
                    <field name="status"/>
                    <field name="method"/>
                    <field name="response_text"/>
                </tree>
            </field>
        </record>

        <record id="action_ridery_log_views" model="ir.actions.act_window" >
            <field name="name">Subject Views</field>
            <field name="res_model">ridery.log</field>
            <field name="view_mode">tree</field>
            <field name="view_id" eval="False"/>
        </record>
    </data>
</odoo>