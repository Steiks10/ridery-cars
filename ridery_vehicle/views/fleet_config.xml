<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="ridery_vehicle_fleet_inherit_setting" model="ir.ui.view">
            <field name="name">ridery_vehicle_fleet_inherit_setting</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="fleet.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='end_contract_setting']" position="after">
                    <div class="row mt16 o_settings_container" id="my_custom_setting">
                        <div class="col-12 col-lg-6 o_setting_box">
                            <div class="o_setting_left_pane"/>
                            <div class="o_setting_right_pane">
                                <span class="o_form_label">Api Key</span>
                                <div class="text-muted content-group mt16">
                                    <field name="app_vehicle_secret" class="text-center" style="width: 50%; min-width: 4rem;"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    </data>
</odoo>